#include <iostream>
#include <unordered_map>
using namespace std;

int main()
{
    int N, M;
    while (cin >> N >> M && N != 0 && M != 0)
    {
        unordered_map<int, int> count;
        int ticket, fakeTickets = 0;

        for (int i = 0; i < M; i++)
        {
            cin >> ticket;
            if (++count[ticket] == 2)
            {
                fakeTickets++;
            }
        }
        cout << fakeTickets << endl;
    }
    return 0;
}
