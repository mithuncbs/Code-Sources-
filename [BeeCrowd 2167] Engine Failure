#include <iostream>
#include <vector>
using namespace std;

int main()
{
    int T;
    cin >> T;
    vector<int> Cases(T);
    for (int i = 0; i < T; i++)
    {
        cin >> Cases[i];
    }

    int dropIndex = 0;
    for (int i = 1; i < T; i++)
    {
        if (Cases[i] < Cases[i - 1])
        {
            dropIndex = i + 1;
            break;
        }
    }
    cout << dropIndex << endl;
    return 0;
}
