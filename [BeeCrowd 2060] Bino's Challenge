#include <iostream>
#include <vector>
using namespace std;

int main()
{
    int N;
    cin >> N;
    while (1 <= N && N <= 1000)
    {
        vector<int> nunmbers(N);
        int temp1 = 0, temp2 = 0, temp3 = 0, temp4 = 0;
        for (int i = 0; i < N; i++)
        {
            cin >> nunmbers[i];
        }
        for (int i = 0; i < N; i++)
        {
            if (nunmbers[i] % 2 == 0)
            {
                temp1++;
            }
            if (nunmbers[i] % 3 == 0)
            {
                temp2++;
            }
            if (nunmbers[i] % 4 == 0)
            {
                temp3++;
            }
            
            if (nunmbers[i] % 5 == 0)
            {
                temp4++;
            }
        }
        cout << temp1 << " Multiplo(s) de 2" << endl;
        cout << temp2 << " Multiplo(s) de 3" << endl;
        cout << temp3 << " Multiplo(s) de 4" << endl;
        cout << temp4 << " Multiplo(s) de 5" << endl;
        break;
    }
    return 0;
}
