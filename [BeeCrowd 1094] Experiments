#include <iostream>
#include <iomanip>
using namespace std;

int main()
{
    int N;
    cin >> N;
    int coelhos = 0, ratos = 0, sapos = 0;
    for (int i = 0; i < N; i++)
    {
        int A;
        char B;
        cin >> A >> B;
        if (B == 'C')
        {
            coelhos += A;
        }
        else if (B == 'R')
        {
            ratos += A;
        }
        else if (B == 'S')
        {
            sapos += A;
        }
    }
    cout << "Total: " << coelhos + ratos + sapos << " cobaias" << endl;
    cout << "Total de coelhos: " << coelhos << endl;
    cout << "Total de ratos: " << ratos << endl;
    cout << "Total de sapos: " << sapos << endl;
    cout << fixed << setprecision(2);
    cout << "Percentual de coelhos: " << (coelhos * 100.0) / (coelhos + ratos + sapos) << " %" << endl;
    cout << "Percentual de ratos: " << (ratos * 100.0) / (coelhos + ratos + sapos) << " %" << endl;
    cout << "Percentual de sapos: " << (sapos * 100.0) / (coelhos + ratos + sapos) << " %" << endl;
    return 0;
}
