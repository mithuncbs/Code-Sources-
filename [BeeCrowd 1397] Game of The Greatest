#include <iostream>
using namespace std;

int main()
{
    int T, A, B, OGsPoint, ChildsPoint;
    while (cin >> T && T != 0)
    {
        OGsPoint = 0;
        ChildsPoint = 0;
        for (int i = 0; i < T; i++)
        {
            cin >> A >> B;
            if (A > B)
                OGsPoint++;
            else if (A < B)
                ChildsPoint++;
        }
        cout << OGsPoint << " " << ChildsPoint << endl;
    }
    return 0;
}
