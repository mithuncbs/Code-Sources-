#include <iostream>
#include <vector>
using namespace std;

int main()
{
    int T, N;
    vector<unsigned long long> fib(61);
    fib[0] = 0;
    fib[1] = 1;
    for (int i = 2; i < 61; ++i)
    {
        fib[i] = fib[i - 1] + fib[i - 2];
    }
    cin >> T;
    while (T--)
    {
        cin >> N;
        cout << "Fib(" << N << ") = " << fib[N] << endl;
    }
    return 0;
}
