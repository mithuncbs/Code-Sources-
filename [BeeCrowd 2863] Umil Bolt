#include <iostream>
#include <vector>
using namespace std;

int main()
{
    int T;
    while (cin >> T && T <= 99 && T >= 2)
    {
        vector<double> A(T);
        double FastestCase = 11;
        for (int i = 0; i < T; i++)
        {
            cin >> A[i];
            if (A[i] < FastestCase)
            {
                FastestCase = A[i];
            }
        }
        cout << FastestCase << endl;
    }
    return 0;
}
