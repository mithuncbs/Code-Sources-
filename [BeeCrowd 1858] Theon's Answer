#include <iostream>
#include <vector>
using namespace std;

int main()
{
    int T;
    cin >> T;
    vector<int>arr(T);
    for (int i = 0; i < T; i++)
    {
        cin >> arr[i];
    }

    int minVal = arr[0];
    int minValIndex = 0;
    for (int i = 1; i < T; i++)
    {
        if (arr[i] < minVal)
        {
            minVal = arr[i];
            minValIndex = i;
        }
    }
    cout << (minValIndex + 1) << endl;
    return 0;
}
