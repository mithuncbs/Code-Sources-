#include <stdio.h>

int main()
{
    for (int i = 1; i < 4; i++)
    {
        printf("I=0 J=%d\n", i);
    }
    for (float i = 0.2; i <= 0.9; i += 0.2)
    {
        for (int j = 1; j < 4; j++)
        {
            printf("I=%.1f J=%.1f\n", i, j + i);
        }
    }
    for (int i = 1; i < 4; i++)
    {
        printf("I=1 J=%d\n", i + 1);
    }
    for (float i = 1.2; i <= 1.9; i += 0.2)
    {
        for (int j = 1; j < 4; j++)
        {
            printf("I=%.1f J=%.1f\n", i, j + i);
        }
    }
    for (int i = 1; i < 4; i++)
    {
        printf("I=2 J=%d\n", i + 2);
    }
    return 0;
}
