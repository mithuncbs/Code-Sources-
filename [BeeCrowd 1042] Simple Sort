#include <stdio.h>

int main()
{
    int num[3], copy[3];

    for (int i = 0; i < 3; i++)
    {
        scanf("%d", &num[i]);
        copy[i] = num[i];
    }
    for (int i = 0; i < 2; i++)
    {
        for (int j = 0; j < 2 - i; j++)
        {
            if (num[j] > num[j + 1])
            {
                int temp = num[j];
                num[j] = num[j + 1];
                num[j + 1] = temp;
            }
        }
    }
    for (int i = 0; i < 3; i++)
    {
        printf("%d\n", num[i]);
    }

    printf("\n");

    for (int i = 0; i < 3; i++)
    {
        printf("%d\n", copy[i]);
    }

    return 0;
}
