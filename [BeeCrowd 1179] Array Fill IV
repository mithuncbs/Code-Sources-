#include <iostream>
using namespace std;

int main()
{
    int evenArr[5], oddArr[5];
    int evenCount = 0, oddCount = 0;
    int number;

    for (int i = 0; i < 15; i++)
    {
        cin >> number;
        if (number % 2 == 0)
        {
            evenArr[evenCount] = number;
            evenCount++;
            if (evenCount == 5)
            {
                for (int i = 0; i < 5; i++)
                {
                    cout << "par[" << i << "] = " << evenArr[i] << endl;
                    evenCount = 0;
                }
            }
        }
        else
        {
            oddArr[oddCount] = number;
            oddCount++;
            if (oddCount == 5)
            {
                for (int i = 0; i < 5; i++)
                {
                    cout << "impar[" << i << "] = " << oddArr[i] << endl;
                    oddCount = 0;
                }
            }
        }
    }
    for (int i = 0; i < oddCount; i++)
    {
        cout << "impar[" << i << "] = " << oddArr[i] << endl;
    }
    for (int i = 0; i < evenCount; i++)
    {
        cout << "par[" << i << "] = " << evenArr[i] << endl;
    }

    return 0;
}
